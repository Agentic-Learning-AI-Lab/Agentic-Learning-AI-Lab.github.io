    "swiper": "^11.1.1",
    "tailwindcss": "^3.4.1",
    "glob": "^7.1.2",
    "handlebars": "^4.7.7",
    "js-yaml": "^3.14.1",
    "moment": "^2.9.0",
    "slugg": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^7.1.3"
